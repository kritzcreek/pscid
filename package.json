{
  "name": "pscid",
  "version": "2.8.0",
  "description": "A lightweight editor experience for PureScript development",
  "repository": "https://github.com/kritzcreek/pscid",
  "bin": {
    "pscid": "index.js"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "purs compile \"src/**/*.purs\" \"bower_components/purescript-*/src/**/*.purs\"",
    "prepack": "rimraf output && npm run compile",
    "build": "npm run -s compile",
    "start": "node index.js"
  },
  "keywords": [
    "IDE",
    "purescript"
  ],
  "author": "kritzcreek",
  "license": "LGPL-3.0",
  "dependencies": {
    "gaze": "^1.1.3",
    "glob": "^7.1.3",
    "keypress": "^0.2.1",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "purescript": "^0.12.3",
    "rimraf": "^2.6.3"
  }
}
